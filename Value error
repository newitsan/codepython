float_1 = 0.25
float_2 = 40.0
product = float(float_1)*float(float_2)
big_string = " The product was " + str(product)
print str(big_string)
product1 = int(float_1)*int(float_2)
small_string = " The second product was " + str(product1)
print (small_string)

#Output:
# The product was 10.0
#The second product was 0
